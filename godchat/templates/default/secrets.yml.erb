<% (['development', 'production'] + [@environment]).uniq.each do |env| -%>
<%= env %>:
  secret_key_base: <%= @secrets[:secret_key_base] %>
  devise_key: <%= @secrets[:devise_key] %>
  pusher_app_id: <%= @secrets[:pusher_app_id] %>
  pusher_key: <%= @secrets[:pusher_key] %>
  pusher_secret: <%= @secrets[:pusher_secret] %>
  missionhub_key: <%= @secrets[:missionhub_key] %>
  missionhub_url: <%= @secrets[:missionhub_url] %>
  errbit_key: <%= @secrets[:errbit_key] %>
  operator_app_url: <%= @secrets[:operator_app_url] %>
  visitor_app_url: <%= @secrets[:visitor_app_url] %>
  fb_app_id: <%= @secrets[:fb_app_id] %>
  fb_app_secret: <%= @secrets[:fb_app_secret] %>
<% end -%>
