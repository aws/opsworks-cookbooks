<dataConfig>
  <dataSource type="JdbcDataSource" driver="<%= @driver %>"
    url="<%= @url %>"
    password="<%= @password %>"
    user="<%= @user %>"
    connectionTimeout="5000" readTimeout="10000"/>
  <document>
    <entity name="search" query="<%= @query %>"
      deltaImportQuery="<%= @deltaImportQuery %>"
      deltaQuery="<%= @deltaQuery %>">
    </entity>
  </document>
</dataConfig>
