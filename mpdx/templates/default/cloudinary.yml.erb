---
<% (['development', 'staging', 'production'] + [@environment]).uniq.each do |env| -%>
<%= env %>:
  cloud_name: <%= @cloudinary[:cloud_name] %>
  api_key: <%= @cloudinary[:api_key] %>
  api_secret: <%= @cloudinary[:api_secret] %>
  enhance_image_tag: <%= @cloudinary[:enhance_image_tag] || true %>
  static_image_support: <%= @cloudinary[:static_image_support] || true %>
<% end -%>
