<%= @videl_env %>:
  senders:
    <%= @name %>:
      <% @conf.each do |vehicle_type, vehicle_type_options| %>
        <%= vehicle_type %>:
          <% vehicle_type_options.each do |type, options| %>
            <%= type %>:
              <% options.each do |key, value| %>
                <% if value.is_a?(Array) %>
                  <%= sprintf("%s: %s", key, value) %>
                <% else %>
                  <%= sprintf("%s: '%s'", key, value) %>
                <% end%>
              <% end %>
          <% end %>
      <% end %>
