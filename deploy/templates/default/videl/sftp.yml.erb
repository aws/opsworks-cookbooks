<%= @videl_env %>:
  senders:
    <%= @name %>:
      type: <%= @conf[:type] %>
      options:
        host: <%= @conf[:options][:host] %>
        user: <%= @conf[:options][:user] %>
        password: <%= @conf[:options][:password] %>
        upload_folder: <%= @conf[:options][:upload_folder] %>
        download_folder: <%= @conf[:options][:download_folder] %>
        port: <%= @conf[:options][:port] %>
        private_key_file: <%= @private_key_file %>
