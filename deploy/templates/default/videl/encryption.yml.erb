<%= @videl_env %>:
  encryption:
    <% @conf.each do |sender, encryption_configs| %>
      <%= sender %>:
        <% encryption_configs.each do |algorithm, algorithm_configs| %>
          <%= algorithm %>:
            <% algorithm_configs.each do |key, value| %>
              <%= sprintf('%s: "%s"', key, value) %>
            <% end %>
      <% end %>
    <% end %>
