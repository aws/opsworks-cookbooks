default: &default

# production and staging env is defined in aws stack settings

<%= @videl_env %>:
  <<: *default
  end_points:
    <% @videl_settings[:hurricane].each do |end_point| %>
    - url: <%= end_point[:url] %>
      origin: <%= end_point[:origin] %>
    <% end %>
  rollbar:
    token: <%= @videl_settings[:rollbar][:token] %>
    enabled: <%= @videl_settings[:rollbar][:enabled] %>
