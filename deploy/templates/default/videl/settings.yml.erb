default: &default
  rollbar:
    token: <%= @videl_settings[:rollbar][:token] %>


# Production env is defined in aws stack settings

production:
  <<: *default
  policy_info_url: <%= @videl_settings[:lumen][:host] %>
  zurich:
    bucket_name: <%= @videl_settings[:bucket][:name] %>
    bucket_region: <%= @videl_settings[:bucket][:region] %>