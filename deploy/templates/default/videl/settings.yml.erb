default: &default

# production and staging env is defined in aws stack settings

<%= @videl_env %>:
  <<: *default
  end_points:
    - url: <%= @videl_settings[:hurricane][:host] %>
      origin: 'H'
    - url: <%= @videl_settings[:hurricane_api][:host] %>
      origin: 'G'
  rollbar:
    token: <%= @videl_settings[:rollbar][:token] %>
    enabled: <%= @videl_settings[:rollbar][:enabled] %>
