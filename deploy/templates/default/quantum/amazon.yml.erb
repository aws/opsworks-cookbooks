default: &default
  access_key_id: <%= @quantum_settings[:access_key_id] %>
  secret_access_key: <%= @quantum_settings[:secret_access_key] %>
  proposals:
    documents_region: <%= @quantum_settings[:proposals][:documents_region] %>
    documents_bucket: <%= @quantum_settings[:proposals][:documents_bucket] %>
    cloudsearch_url: <%= @quantum_settings[:proposals][:cloudsearch_url] %>
  claims:
    documents_region: <%= @quantum_settings[:claims][:documents_region] %>
    documents_bucket: <%= @quantum_settings[:claims][:documents_bucket] %>

staging:
  <<: *default

production:
  <<: *default
