default: &default
  proposals:
    documents_bucket:
      region: <%= @quantum_settings[:proposals][:documents_region] %>
      name: <%= @quantum_settings[:proposals][:documents_bucket] %>
  claims:
    documents_bucket:
      region: <%= @quantum_settings[:claims][:documents_region] %>
      name: <%= @quantum_settings[:claims][:documents_bucket] %>
    queue:
      url: <%= @quantum_settings[:claims][:queue_url] %>

staging:
  <<: *default

production:
  <<: *default
