default: &default
  product_guarantee_fallback: <%= @etl_settings[:product_guarantee_fallback] %>

<%= @etl_env %>:
  <<: *default
  rollbar:
    token: <%= @etl_settings[:rollbar][:token] %>
    enabled: <%= @etl_settings[:rollbar][:enabled] %>
  hyena:
    host: <%= @etl_settings[:hyena][:host] %>
    username: <%= @etl_settings[:hyena][:username] %>
    password: <%= @etl_settings[:hyena][:password] %>
    database: <%= @etl_settings[:hyena][:database] %>

  joomla:
    host: <%= @etl_settings[:joomla][:host] %>
    username: <%= @etl_settings[:joomla][:username] %>
    password: <%= @etl_settings[:joomla][:password] %>
    database: <%= @etl_settings[:joomla][:database] %>

  quantum:
    host: <%= @etl_settings[:quantum][:host] %>
    user: <%= @etl_settings[:quantum][:username] %>
    password: <%= @etl_settings[:quantum][:password] %>
    dbname: <%= @etl_settings[:quantum][:database] %>

  hurricane:
    export_quantum_url: <%= @etl_settings[:hurricane][:export_quantum_url] %>
