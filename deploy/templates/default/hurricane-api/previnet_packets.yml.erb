default: &default
  packets:
    <% @hurricane_api_settings[:previnet_packets][:packets].each do |packet| %>
    - name: <%= packet[:name] %>
      products:
        <% packet[:products].each do |product| %>
        - <%= product %>
        <% end %>
    <% end %>

<%= @env %>:
    <<: *default

test:
    <<: *default
